<idea-plugin version="2">
    <id>krasa.IdeaIconPack</id>
    <name>Legacy Icon Pack for IntelliJ 2018.2+</name>
    <version>1.5</version>
    <vendor url="https://github.com/krasa/IdeaIconPack" email="vojta.krasa@gmail.com">Vojtech Krasa</vendor>

    <description>
        <![CDATA[Changes new modern icons to the old ones. You can choose between icons from IntelliJ 2016.2 (includes some from 2018.1) and 2018.1. 
         <br/>
         <br/>
        
        Based on <a href="https://plugins.jetbrains.com/plugin/7285-idea-2016-2-icon-pack">Idea 2016.2 Icon Pack</a> plugin.]]></description>

    <change-notes>
    </change-notes>
    <idea-version since-build="180.000"/>
    <depends>com.intellij.modules.lang</depends>

    <application-components>
        <component>
            <implementation-class>krasa.iconpack.Patcher</implementation-class>
        </component>
    </application-components>

    <project-components>
    </project-components>

    <actions>
    </actions>

    <extensions defaultExtensionNs="com.intellij">
        <applicationConfigurable id="krasa.IdeaIconPack" instance="krasa.iconpack.MyConfigurable"
                                 displayName="Legacy Icon Pack"/>
    </extensions>
</idea-plugin>